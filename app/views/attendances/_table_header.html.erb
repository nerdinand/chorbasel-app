<thead>
  <tr class="sticky top-0 z-10">
    <th><%= t('.name') %></th>
    <% attendance_table.calendar_events.each do |calendar_event| %>
      <th class="<%= calendar_event.practice? ? '' : 'bg-complementary bg-opacity-30' %>">
        <%= link_to calendar_event.date, calendar_event %>
      </th>
    <% end %>
  </tr>
</thead>
